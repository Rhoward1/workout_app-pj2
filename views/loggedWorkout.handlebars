<div class="add-logged-workout">
    <h2>
      Log your workout below!
    </h2>
    <form class="new-log-form">
      <label for="length">
        Length of Workout:
      </label>
      <br />
      <textarea id="length" name="length" rows="2" cols="30">
      {{length}}
    </textarea>
      <br />
      <label for="rating">
        Rate your workout 1-5:
      </label>
      <br />
      <textarea id="rating" name="rating" rows="5" cols="30">
      {{rating}}
    </textarea>
      <br />
      <button type="submit" value="Submit">
        Submit
      </button>
    </form>
    </div>
    <script src="../public/js/add-log.js"></script>